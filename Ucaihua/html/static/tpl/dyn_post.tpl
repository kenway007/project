<div class="dyn-post">
    <div class="dp-content-box">
        <div class="kw-wp dp-content-fix">
            <textarea placeholder="请填写需求..." name="content"  class="dp-content" ></textarea>
        </div>
        <div class="dyn-post-img kw-upload-box"></div>
        <div class="kw-wp dyn-post-ctr">
            <div class="fa dpc-item fa-camera dp-camera">
                <input multiple class="dp-camera-input kw-upload-file" type="file"  value=""/>
            </div>
            <div class="fa dpc-item fa-map-marker dp-position"><span class="dp-area-name"><%- common.comp_address() %></span></div>
        </div>
    </div>
</div>


<div class="pc-fixed btn-green btn-bf kw-f-bottom dyn-post-ok">发布</div>


<div class="dpp-popup pc-fixed">
    <div class="kw-wp">
        <div class="dpp-li dpp-popup-del">删除地址</div>
        <div class="dpp-li dpp-popup-reget">重新获取</div>
        <div class="dpp-li dpp-popup-cancel">取消</div>
    </div>
</div>